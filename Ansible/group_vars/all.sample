def_ssh_timeout: 10
destroy_forced: no
def_env_uuid: "unknown"

# Environment database settings
def_env_db_name: "trillian_envs"
def_env_db_ip: "<IP_ADDRESS_HERE>"

# Environment summary
def_env_zonetype: "Advanced"
def_env_num_pods: 1
