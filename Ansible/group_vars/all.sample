# Defaults for generate-config:
mgmt: 1
db: 0
sec: 1
def_build_marvin: no
wait_till_setup: no
def_api_retries: 25
def_sysvms_running_retries: 250
def_sysvms_up_retries: 100
def_builin_template_retries: 400
def_ui_up_retries: 75
